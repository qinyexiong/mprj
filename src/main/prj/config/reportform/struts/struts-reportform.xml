<struts>
	<package name="reportform" namespace="/reportforms" extends="qyx-default">
		<action name="reportform_*" class="main.prj.java.com.web.demo.action.ReportFormAction" method="{1}">
			<result name="userform">/WEB-INF/page/reportform/userform.jsp</result>
			<result name="success" type="json"> 
